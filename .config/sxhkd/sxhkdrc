super + e
	$TERMINAL ranger

super + o
	opera

super + Return
	$TERMINAL

super + Escape
	~/scripts/xkcd_lock.sh

super + n
	$TERMINAL ~/scripts/neofetch.sh

super + s 
	~/scripts/dscript.sh

super + c
	$TERMINAL tty-clock -cs

super + F1
	$TERMINAL ranger /run/media/nevimmu/

super +F2
	$TERMINAL joplin

super + F12
	networkmanager_dmenu

## Media keys

XF86AudioRaiseVolume
	pactl set-sink-volume 0 +5%

XF86AudioLowerVolume
	pactl set-sink-volume 0 -5%

XF86AudioMute
	amixer -q set Master toggle

XF86MonBrightnessDown
	xbacklight -dec 10
 
XF86MonBrightnessUp
	xbacklight -inc 10

Print
	scrot -z ~/Pictures/Screenshots/screenshot-$(date '+%y%m%d-%H%M-%S').png
